<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>olahLogin</servlet-name>
        <servlet-class>web.olahLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateBarangAction</servlet-name>
        <servlet-class>web.editBarangAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tambahBarangAction</servlet-name>
        <servlet-class>web.tambahBarangAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteBarangAction</servlet-name>
        <servlet-class>web.deleteBarangAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditBarangAction</servlet-name>
        <servlet-class>web.EditBarangAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TambahStok</servlet-name>
        <servlet-class>web.TambahStok</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteBarangKeranjang</servlet-name>
        <servlet-class>web.deleteBarangKeranjang</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tambahKeranjang</servlet-name>
        <servlet-class>web.tambahKeranjang</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>selesaiTransaksi</servlet-name>
        <servlet-class>web.selesaiTransaksi</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>olahLogin</servlet-name>
        <url-pattern>/olahLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateBarangAction</servlet-name>
        <url-pattern>/updateBarangAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tambahBarangAction</servlet-name>
        <url-pattern>/tambahBarangAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteBarangAction</servlet-name>
        <url-pattern>/deleteBarangAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditBarangAction</servlet-name>
        <url-pattern>/EditBarangAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TambahStok</servlet-name>
        <url-pattern>/TambahStok</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteBarangKeranjang</servlet-name>
        <url-pattern>/deleteBarangKeranjang</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tambahKeranjang</servlet-name>
        <url-pattern>/tambahKeranjang</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>selesaiTransaksi</servlet-name>
        <url-pattern>/selesaiTransaksi</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
